{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (console)",
            "type": "clr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/DatasetRefactor.UI/bin/Debug/net472/DatasetRefactor.UI.exe",
            "args": [
                "-dataset=DATASET_NAME_FILTER",
                "-source=PATH_TO_DATASET_DLL"
            ],
            "cwd": "${workspaceFolder}/src/DatasetRefactor.UI",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Attach",
            "type": "clr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}